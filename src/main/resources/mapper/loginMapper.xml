<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

 
<mapper namespace="org.hello.mapper.LoginMapper">
	<select id="loginRequest" parameterType = "org.hello.vo.MemberVo" resultType = "org.hello.vo.MemberVo">
		select * from nc_b_user_t where USERID=#{userId} and PASSWD=#{passWD}
	</select>
</mapper> 