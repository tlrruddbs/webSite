<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

 				
<mapper namespace="org.hello.mapper.CommonCodeMapper">
	<select id = "commonCode" parameterType = "String" resultType="String">
		SELECT CODE FROM common_code WHERE CODE_VALUE = #{codeValue}	
	</select>
	 
	<select id = "commonCodeValue" parameterType = "String" resultType="String">
		SELECT CODE_VALUE FROM common_code WHERE CODE = #{code}	
	</select>
	
	 
</mapper>